<?xml version="1.0" encoding="UTF-8"?>

<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
  
  <grid model="com.axelor.student.db.School" title="Schools" name="school-grid" customSearch="false"  >
  <!-- <grid model="com.axelor.student.db.School" title="Schools" name="school-grid" canArchive="false" canDelete="false" canEdit="false"  edit-icon="false" canNew="false"  editable="true" canSave="false" x-no-fetch="false"> --> 
  	<field name="schoolType"/>
  	<field name="schoolName"/>
  	<field name="address"/>
  </grid>
  
  <grid model="com.axelor.student.db.School" title="Schools" name="school-test-grid" editable="true">
  	<field name="schoolType" title="Test school"/>
  	<field name="schoolName"/>
  	<field name="address" />
  </grid>
 <form model="com.axelor.student.db.School" title="School" name="school-form">
 	<panel>
 		<field name="schoolType"/>
 		<field name="schoolName"/>
 		<field name="address"/>
 		
 		<panel-related field="studentList"/><!-- for o2m & m2m we have to use panel related  -->
 		<panel-related field="peopleSet"/>
 	</panel>
 </form>
  
  
</object-views>
  